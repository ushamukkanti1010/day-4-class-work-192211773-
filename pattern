#include <stdio.h>

int main() {
  int i, j;
  
  // print the top half of the pattern
  for (i = 1; i <= 4; i++) {
    for (j = 1; j <= i; j++) {
      printf("%d", i);
    }
    printf("\n");
  }
  
  // print the bottom half of the pattern
  for (i = 3; i >= 1; i--) {
    for (j = 1; j <= i; j++) {
      printf("%d", i);
    }
    printf("\n");
  }
  
  return 0;
}
